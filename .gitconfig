[user]
	name = Jerome Basa
	email = jeromebasa@gmail.com
[push]
  default = tracking
[branch]
  autosetuprebase = always
[core]
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
	excludesfile = /Users/jerome/.gitignore
[alias]
  co = checkout
  br = branch
[apply]
  whitespace = nowarn
[hub]
  protocol = https
[push]
  default = tracking
[branch]
  autosetuprebase = always
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path =
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[github]
	user = jldbasa
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
	process = git-lfs filter-process
[color]
  ui = auto
[color "branch"]
  current = yellow
  local = 241
  remote = green
[color "diff"]
  meta = yellow
  frag = magenta
  old = red
  new = green
[color "status"]
  added = green
  changed = yellow
  untracked = magenta
[web]
	browser = open
[pager]
	branch = false
  tag = false
